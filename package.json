{
  "name": "vscode-go-work-manager",
  "displayName": "go.work manager",
  "description": "manage your go workspace",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/dashengyeah/vscode-go-work-manager"
  },
  "publisher": "dashengyeah",
  "homepage": "https://github.com/dashengyeah/vscode-go-work-manager",
  "bugs": {
    "url": "https://github.com/dashengyeah/vscode-go-work-manager/issues",
    "email": "dashengyeah@hotmail.com"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:go.mod"
  ],
  "keywords": [
    "gowork",
    "go.work"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-go-work-manager.hello",
        "title": "Hello go.work"
      },
      {
        "command": "vscode-go-work-manager.toggleItem",
        "title": "Toggle",
        "icon": {
          "light": "media/toggle.svg",
          "dark": "media/toggle.svg"
        }
      },
      {
        "command": "vscode-go-work-manager.testReload",
        "title": "Test Reload"
      },
      {
        "command": "vscode-go-work-manager.testAdd",
        "title": "Test Add"
      },
      {
        "command": "vscode-go-work-manager.testDrop",
        "title": "Test Drop"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "goWorkManager",
          "name": "Go Work"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "vscode-go-work-manager.toggleItem",
          "when": "view == goWorkManager && viewItem == goModItem",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "esbuild": "^0.15.7",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}